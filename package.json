{
  "name": "nextjs-auth-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "list-routes": "node scripts/listRoutes.js",
    "test:openfga": "./tests/openfga.sh",
    "openfga:model": "fga model transform --file='openfga/model.fga' | jq > 'openfga/model.json'",
    "openfga:check": "DOTENV_CONFIG_PATH='.env.local' node -r dotenv/config scripts/openfga/check.mjs",
    "openfga:write": "DOTENV_CONFIG_PATH='.env.local' node -r dotenv/config scripts/openfga/write.mjs",
    "openfga:read-all": "DOTENV_CONFIG_PATH='.env.local' node -r dotenv/config scripts/openfga/read-all.mjs",
    "openfga:seed": "DOTENV_CONFIG_PATH='.env.local' node -r dotenv/config scripts/openfga/seed.mjs"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.7.0",
    "@openfga/sdk": "^0.2.6",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}